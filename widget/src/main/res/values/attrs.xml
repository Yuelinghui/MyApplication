<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerPlayView">
        <attr name="autoScrollEvent" format="integer"></attr>
        <attr name="pageNumber" format="integer"></attr>
        <attr name="pageDrawableNormal" format="reference"></attr>
        <attr name="pageDrawableSelected" format="reference"></attr>
        <attr name="pageMarginBottom" format="dimension"></attr>
        <attr name="pageInterval" format="dimension"></attr>
        <attr name="pageDotSize" format="dimension"></attr>
        <attr name="belongType" format="string"></attr>
        <attr name="scrollDuration" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="QuickSideBarView">
        <attr name="sidebarTextColor" format="color|reference"/>
        <attr name="sidebarBackgroundColor" format="color|reference"/>
        <attr name="sidebarTextColorChoose" format="color|reference"/>
        <attr name="sidebarTextSize" format="dimension"/>
        <attr name="sidebarTextSizeChoose" format="dimension"/>
        <attr name="sidebarItemHeight" format="dimension"/>
    </declare-styleable>
</resources>